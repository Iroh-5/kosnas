entities:

- name: nas.FsReaderEntity
  path: FsReaderEntity
  connections:
  - target: nas.FsReaderVfs
    id: {var: _VFS_CONNECTION_ID, include: vfs/defs.h}
@INIT_FsReader_ENTITY_ENV@

- name: nas.FsWriterEntity
  path: FsWriterEntity
  connections:
  - target: nas.FsWriterVfs
    id: {var: _VFS_CONNECTION_ID, include: vfs/defs.h}
@INIT_FsWriter_ENTITY_ENV@

- name: nas.FsProxyEntity
  path: FsProxyEntity
  connections:
  - target: nas.FsReaderEntity
    id: FsReaderConnection
  - target: nas.FsWriterEntity
    id: FsWriterConnection
@INIT_FsReaderVfs_ENTITY_CONNECTIONS+@

- name: nas.FsReaderVfs
  path: FsReaderVfs
@INIT_FsReaderVfs_ENTITY_CONNECTIONS@

- name: nas.FsWriterVfs
  path: FsWriterVfs
@INIT_FsReaderVfs_ENTITY_CONNECTIONS@

@INIT_EXTERNAL_ENTITIES@
