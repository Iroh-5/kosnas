entities:

- name: nas.FsReader
  path: FsReader
  connections:
  - target: @ROOTFS_ENTITY@
    id: @ROOTFS_ENTITY@
  env:
    VFS_FILESYSTEM_BACKEND: client:@ROOTFS_ENTITY@
@INIT_FsReader_ENTITY_ENV@

- name: nas.FsWriter
  path: FsWriter
  connections:
  - target: @ROOTFS_ENTITY@
    id: @ROOTFS_ENTITY@
  env:
    VFS_FILESYSTEM_BACKEND: client:@ROOTFS_ENTITY@
@INIT_FsWriter_ENTITY_ENV@

@INIT_EXTERNAL_ENTITIES@
